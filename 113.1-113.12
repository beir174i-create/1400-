# П3.10
A = False
B = False
C = True

print("П3.10:")
print("а) (не А или не В) и не С:", ((not A) or (not B)) and (not C))  # (True or True) and False = False
print("б) (не А или не В) и (А или В):", ((not A) or (not B)) and (A or B))  # (True or True) and False = False
print("в) А и В или А и С или не С:", (A and B) or (A and C) or (not C))  # False or False or False = False
print()

# П3.11
x, y = 1, -1
print("П3.11:")
print("а) x^2 + y^2 ≤ 4 при x=1, y=-1:", x**2 + y**2 <= 4)  # 1 + 1 <= 4 → True
x, y = 1, 2
print("б) (x ≥ 0) или (y^2 ≠ 4):", (x >= 0) or (y**2 != 4))  # True or False = True
print("в) (x ≥ 0) и (y^2 ≠ 4):", (x >= 0) and (y**2 != 4))    # True and False = False
x, y = 2, 1
print("г) (x*y ≠ 0) и (y > x):", (x * y != 0) and (y > x))    # True and False = False
print("д) (x*y ≠ 0) или (y < x):", (x * y != 0) or (y < x))   # True or True = True
print("е) (не (x*y < 0)) и (y > x):", (not (x * y < 0)) and (y > x))  # True and False = False
x, y = 1, 2
print("ж) (не (x*y < 0)) или (y > x):", (not (x * y < 0)) or (y > x)) # True or True = True
print()

