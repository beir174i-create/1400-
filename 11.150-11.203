    i = 0
    
    while i < n:
        if arr[i] % a == 0:
            # Вставляем перед текущим элементом
            arr.append(0)
            for j in range(n, i, -1):
                arr[j] = arr[j-1]
            arr[i] = num
            n += 1
            i += 2  # Пропускаем вставленный элемент и текущий (который сдвинулся)
        else:
            i += 1
    
    return arr

