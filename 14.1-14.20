14.9. Равнобедренная трапеция - периметр и площадь
python
import math

def trapezoid_info(base1, base2, height):
    """
    Вычисляет периметр и площадь равнобедренной трапеции
    base1 - меньшее основание
    base2 - большее основание
    height - высота
    """
    # Боковая сторона (по теореме Пифагора)
    side = math.sqrt(((base2 - base1) / 2)**2 + height**2)
    
    # Периметр
    perimeter = base1 + base2 + 2 * side
    
    # Площадь
    area = (base1 + base2) * height / 2
    
    return perimeter, area, side

# Пример использования
base1_1, base2_1, height_1 = 5, 10, 4
base1_2, base2_2, height_2 = 3, 8, 5

p1, s1, side1 = trapezoid_info(base1_1, base2_1, height_1)
p2, s2, side2 = trapezoid_info(base1_2, base2_2, height_2)

print(f"Трапеция 1: основания {base1_1}/{base2_1}, высота {height_1}")
print(f"  Боковая сторона: {side1:.2f}")
print(f"  Периметр: {p1:.2f}")
print(f"  Площадь: {s1:.2f}")

print(f"\nТрапеция 2: основания {base1_2}/{base2_2}, высота {height_2}")
print(f"  Боковая сторона: {side2:.2f}")
print(f"  Периметр: {p2:.2f}")
print(f"  Площадь: {s2:.2f}")

print(f"\nСумма периметров: {p1 + p2:.2f}")
print(f"Сумма площадей: {s1 + s2:.2f}")
