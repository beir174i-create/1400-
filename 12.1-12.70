var
  sentence: string;
  i: integer;
  hasComma: boolean;
begin
  write('Введите предложение: ');
  readln(sentence);
  
  hasComma := false;
  for i := 1 to length(sentence) do
    if sentence[i] = ',' then
      hasComma := true;
      
  if hasComma then
    writeln('В предложении есть запятые')
  else
    writeln('В предложении нет запятых');
end.


