  text: string;
  i, countPlus, countStar: integer;
begin
  write('Введите текст: ');
  readln(text);
  
  countPlus := 0;
  countStar := 0;
  for i := 1 to length(text) do
  begin
    if text[i] = '+' then
      countPlus := countPlus + 1;
    if text[i] = '*' then
      countStar := countStar + 1;
  end;
      
  writeln('Символов "+": ', countPlus);
  writeln('Символов "*": ', countStar);
end.
