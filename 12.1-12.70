var
  sentence: string;
  i, countO, countA: integer;
begin
  write('Введите предложение: ');
  readln(sentence);
  
  countO := 0;
  countA := 0;
  for i := 1 to length(sentence) do
  begin
    if sentence[i] = 'о' then
      countO := countO + 1;
    if sentence[i] = 'а' then
      countA := countA + 1;
  end;
      
  if countO > countA then
    writeln('Буква "о" встречается чаще')
  else if countA > countO then
    writeln('Буква "а" встречается чаще')
  else
    writeln('Буквы "о" и "а" встречаются одинаково часто');
end.
