def remove_max_unique(arr):
    if arr and len(set(arr)) == len(arr):  # все элементы различны
        max_index = arr.index(max(arr))
        for i in range(max_index, len(arr)-1):
            arr[i] = arr[i+1]
        arr[-1] = 0
    return arr

# б) удалить минимальный элемент
def remove_min_unique(arr):
    if arr and len(set(arr)) == len(arr):
        min_index = arr.index(min(arr))
        for i in range(min_index, len(arr)-1):
            arr[i] = arr[i+1]
        arr[-1] = 0
    return arr
