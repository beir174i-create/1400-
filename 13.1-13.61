python
# 13.53. Обозначение элемента по названию
elements = {
    'медь': 'Cu',
    'кислород': 'O', 
    'водород': 'H',
    'азот': 'N',
    'железо': 'Fe',
    'золото': 'Au',
    'серебро': 'Ag'
}

def get_symbol_by_name(element_name):
    """13.53: Обозначение по названию"""
    return elements.get(element_name, "Элемент не найден")

# 13.54. Название элемента по обозначению
def get_name_by_symbol(symbol):
    """13.54: Название по обозначению"""
    for name, sym in elements.items():
        if sym == symbol:
            return name
    return "Обозначение не найдено"

# 13.55. Проверка наличия названия
def check_name_exists(name):
    """13.55: Проверка наличия названия"""
    return name in elements

# 13.56. Проверка наличия обозначения
def check_symbol_exists_v1(symbol):
    """13.56 (вариант 1): с использованием values()"""
    return symbol in elements.values()

def check_symbol_exists_v2(symbol):
    """13.56 (вариант 2): без использования values()"""
    for sym in elements.values():
        if sym == symbol:
            return True
    return False

# Демонстрация работы
