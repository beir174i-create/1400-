def find_x_from_n(n):
    last_digit = n % 10
    remaining_number = (n - last_digit) // 10
    for x in range(100, 1000):
        if (x // 10) % 10 == last_digit and x // 100 * 10 + (x % 10) == remaining_number:
            return x

