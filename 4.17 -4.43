import math

def can_circle_fit_in_square(circle_area, square_area):
    circle_radius = math.sqrt(circle_area / math.pi)
    square_side = math.sqrt(square_area)
    return circle_radius <= square_side / 2

def can_square_fit_in_circle(circle_area, square_area):
    circle_radius = math.sqrt(circle_area / math.pi)
    square_side = math.sqrt(square_area)
    return square_side <= circle_radius * 2

