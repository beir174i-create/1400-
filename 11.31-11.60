def task_11_42(arr, k1=None, k2=None, s1=None, s2=None):
    """Различные суммы и произведения элементов массива"""
    n = len(arr)
    
    # а) Сумма всех элементов
    total_sum = sum(arr)
    
    # б) Произведение всех элементов
    product = 1
    for num in arr:
        product *= num
    
    # в) Сумма квадратов
    sum_of_squares = sum(x**2 for x in arr)
    
    # г) Сумма шести первых элементов
    sum_first_six = sum(arr[:6]) if n >= 6 else sum(arr)
    
    # д) Сумма с k1 по k2
    if k1 is not None and k2 is not None and 0 <= k1 <= k2 < n:
        sum_k1_k2 = sum(arr[k1:k2+1])
    else:
        sum_k1_k2 = None
    
    # е) Среднее арифметическое всех элементов
    average_all = total_sum / n if n > 0 else 0
    
    # ж) Среднее арифметическое с s1 по s2
    if s1 is not None and s2 is not None and 0 <= s1 <= s2 < n:
        avg_s1_s2 = sum(arr[s1:s2+1]) / (s2 - s1 + 1)
    else:
        avg_s1_s2 = None
    
    return {
        'total_sum': total_sum,
        'product': product,
        'sum_of_squares': sum_of_squares,
        'sum_first_six': sum_first_six,
        'sum_k1_k2': sum_k1_k2,
        'average_all': average_all,
        'avg_s1_s2': avg_s1_s2
    }

