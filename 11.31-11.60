import math

def task_11_40():
    """а) Квадратный корень из любого элемента
       б) Среднее арифметическое двух любых элементов"""
    
    def sqrt_of_element(arr, index):
        """а) Квадратный корень из элемента с индексом index"""
        if 0 <= index < len(arr):
            return math.sqrt(arr[index]) if arr[index] >= 0 else math.nan
        return None
    
    def average_of_two(arr, index1, index2):
        """б) Среднее арифметическое двух элементов"""
        if 0 <= index1 < len(arr) and 0 <= index2 < len(arr):
            return (arr[index1] + arr[index2]) / 2
        return None
    
    return sqrt_of_element, average_of_two

