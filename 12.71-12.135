  readln(sentence);
  
  firstComma := pos(',', sentence);
  if firstComma > 0 then
  begin
    // Ищем вторую запятую
    secondComma := pos(',', copy(sentence, firstComma+1, length(sentence))) + firstComma;
    
    if secondComma > firstComma then
    begin
      writeln('Символы между первой и второй запятыми:');
      for i := firstComma+1 to secondComma-1 do
        write(sentence[i]);
      writeln;
    end
    else
    begin
      writeln('Второй запятой нет. Символы после единственной запятой:');
      for i := firstComma+1 to length(sentence) do
        write(sentence[i]);
      writeln;
    end;
  end
  else
    writeln('В предложении нет запятых');
end.
