            elif goals_for == goals_against:
                draws += 1
            else:
                losses += 1
    
    print(f"{teams[i]}: {wins} побед, {draws} ничьих, {losses} поражений")

# д) Очки каждой команды (без использования сводной таблицы)
print("\nОчки команд:")
for i in range(n):
    points = 0
    for j in range(n):
        if i != j:
            goals_for, goals_against = detailed_results[i][j-1 if j>i else j]
            if goals_for > goals_against:
                points += 3
            elif goals_for == goals_against:
                points += 1
    
    print(f"{teams[i]}: {points} очков")

# е) Чемпион (наибольшее количество очков)
team_points = []
for i in range(n):
    points = 0
    for j in range(n):
