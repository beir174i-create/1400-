# а) удалить четные элементы на нечетных местах
def remove_even_on_odd_positions(arr):
    i = 0
    count = 0
    n = len(arr)
    
    while i < n - count:
        # Нечетные позиции имеют четные индексы (0-based)
        if i % 2 == 0 and arr[i] % 2 == 0:
            for j in range(i, n-1-count):
                arr[j] = arr[j+1]
            arr[n-1-count] = 0
            count += 1
        else:
            i += 1
    
    return arr

# б) удалить все элементы, кратные 3 или 5
def remove_multiples_of_3_or_5(arr):
    i = 0
    count = 0
    n = len(arr)
    
    while i < n - count:
        if arr[i] % 3 == 0 or arr[i] % 5 == 0:
            for j in range(i, n-1-count):
                arr[j] = arr[j+1]
            arr[n-1-count] = 0
            count += 1
        else:
            i += 1
    
    return arr
11.165. Удалить повторяющиеся элементы
python
def remove_duplicates(arr):
    seen = set()
    i = 0
    count = 0
    n = len(arr)
    
    while i < n - count:
        if arr[i] in seen:
            for j in range(i, n-1-count):
                arr[j] = arr[j+1]
            arr[n-1-count] = 0
            count += 1
        else:
            seen.add(arr[i])
            i += 1
    
    return arr
