12.35
pascal
var
  word, part1, part2: string;
  i: integer;
begin
  write('Введите слово (четное количество букв): ');
  readln(word);
  
  if length(word) mod 2 = 0 then
  begin
    // Способ 1: без цикла
    part1 := copy(word, 1, length(word) div 2);
    part2 := copy(word, length(word) div 2 + 1, length(word) div 2);
    writeln('Способ 1: ', part2 + part1);
    
    // Способ 2: с циклом
    for i := 1 to length(word) div 2 do
    begin
      part1 := part1 + word[i];
      part2 := part2 + word[i + length(word) div 2];
    end;
    writeln('Способ 2: ', part2 + part1);
  end
  else
    writeln('Слово имеет нечетное количество букв');
end.
