def insert_two_numbers(arr, n, m):
    # Вставляем n перед элементами > n
    length = len(arr)
    i = 0
    
    while i < length:
        if arr[i] > n:
            arr.append(0)
            for j in range(length, i, -1):
                arr[j] = arr[j-1]
            arr[i] = n
            length += 1
            i += 2
        else:
            i += 1
    
    # Вставляем m после элементов < m
    i = 0
    length = len(arr)
    
    while i < length:
        if arr[i] < m:
            arr.append(0)
            for j in range(length, i, -1):
                arr[j] = arr[j-1]
            arr[i+1] = m
            length += 1
            i += 2
        else:
            i += 1
    
    return arr
