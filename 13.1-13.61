# Главная диагональ = 0 (команда не играет сама с собой)
results = [
    [0, 3, 1, 0],  # Команда A
    [0, 0, 3, 1],  # Команда B  
    [1, 0, 0, 3],  # Команда C
    [3, 1, 0, 0]   # Команда D
]

n = len(teams)

# а) Команды с большим количеством побед, чем поражений
print("Команды с большим количеством побед:")
for i in range(n):
    wins = sum(1 for j in range(n) if results[i][j] == 3)
    losses = sum(1 for j in range(n) if results[i][j] == 0 and i != j)
    if wins > losses:
        print(f"{teams[i]}: {wins} побед, {losses} поражений")

# б) Команды без поражений
print("\nКоманды без поражений:")
for i in range(n):
    has_loss = any(results[i][j] == 0 for j in range(n) if i != j)
    if not has_loss:
        print(teams[i])

# в) Чемпион (наибольшее количество очков)
team_points = []
for i in range(n):
    points = sum(results[i][j] for j in range(n))
    team_points.append((teams[i], points))

champion = max(team_points, key=lambda x: x[1])
