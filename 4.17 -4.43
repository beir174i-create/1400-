def traffic_light(t):
    t = t % 8  # 3 minutes green + 2 minutes red + 3 minutes green = 8 minutes cycle
    if t < 3:
        return "Зеленый"
    elif t < 5:
        return "Красный"
    else:
        return "Зеленый"

