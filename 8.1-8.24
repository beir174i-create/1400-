    worker_total = sum(salaries[i])
    print(f"Работник {i+1}: {worker_total:.2f}")

# в) Общая зарплата всех работников за каждый месяц
print("\nв) Общая зарплата за каждый месяц:")
for j in range(months):
    month_total = 0
    for i in range(workers):
        month_total += salaries[i][j]
    print(f"Месяц {j+1}: {month_total:.2f}")
