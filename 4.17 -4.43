def bounding_rectangle(x1_1, y1_1, x2_1, y2_1, x1_2, y1_2, x2_2, y2_2):
    left_bottom_x = min(x1_1, x1_2)
    left_bottom_y = min(y1_1, y1_2)
    right_top_x = max(x2_1, x2_2)
    right_top_y = max(y2_1, y2_2)
    return left_bottom_x, left_bottom_y, right_top_x, right_top_y

