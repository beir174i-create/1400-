def bounding_rectangle(x1, y1, width1, height1, x2, y2, width2, height2):
    x2_1 = x1 + width1
    y2_1 = y1 + height1
    x2_2 = x2 + width2
    y2_2 = y2 + height2

    left_bottom_x = min(x1, x2)
