def move_first_k_to_end(arr, k):
    n = len(arr)
    if n == 28 and 0 < k < n:
        # Сохраняем первые k элементов
        temp = arr[:k]
        
        # Сдвигаем остальные элементы
        for i in range(n - k):
            arr[i] = arr[i + k]
        
        # Восстанавливаем первые k в конце
        for i in range(k):
            arr[n - k + i] = temp[i]
    
    return arr
