11.154. Переставить элементы в обратном порядке
python
# а) между вторым и десятым (с 3 по 9)
def reverse_between_2_10(arr):
    if len(arr) >= 10:
        # Индексы: второй элемент - индекс 1, десятый - индекс 9
        # Между ними: индексы 2-8
        arr[2:9] = arr[8:1:-1]
    return arr

# б) между k-м и s-м (k < s)
def reverse_between_k_s(arr, k, s):
    if 0 <= k < s < len(arr):
        # Элементы между: с k+1 по s-1
        arr[k+1:s] = arr[s-1:k:-1]
    return arr

# в) между максимальным и минимальным
def reverse_between_max_min(arr):
    if arr:
        max_index = arr.index(max(arr))
        min_index = arr.index(min(arr))
        
        # Определяем границы
        start = min(max_index, min_index)
        end = max(max_index, min_index)
        
        # Разворачиваем включая границы
        arr[start:end+1] = arr[end:start-1:-1] if start > 0 else arr[end::-1]
    return arr
11.155. Поменять местами первый отрицательный и последний положительный
python
def swap_first_neg_last_pos(arr):
    first_neg = None
    last_pos = None
    
    # Находим первый отрицательный
    for i in range(len(arr)):
        if arr[i] < 0:
            first_neg = i
            break
    
    # Находим последний положительный
    for i in range(len(arr)-1, -1, -1):
        if arr[i] > 0:
            last_pos = i
            break
    
    # Меняем местами, если оба найдены
    if first_neg is not None and last_pos is not None:
        arr[first_neg], arr[last_pos] = arr[last_pos], arr[first_neg]
    
    return arr
