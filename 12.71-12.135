var
  word, middle: string;
  k, s, i: integer;
begin
  write('Введите слово из 15 букв: ');
  readln(word);
  write('Введите k (k < s): ');
  readln(k);
  write('Введите s: ');
  readln(s);
  
  if (length(word) = 15) and (k >= 1) and (s <= 15) and (k < s) then
  begin
    // Берем буквы с k+1 по s-1
    middle := copy(word, k+1, s-k-1);
    
    // Переворачиваем их
    for i := 1 to (s-k-1) div 2 do
    begin
      middle[i] := word[s-i];
      middle[s-k-1-i+1] := word[k+i];
    end;
    
    // Собираем слово обратно
    word := copy(word, 1, k) + middle + copy(word, s, length(word)-s+1);
    writeln('Результат: ', word);
  end
  else
    writeln('Некорректные данные');
end.
