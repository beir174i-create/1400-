# П3.5
A = True
B = False
C = False

print("П3.5:")
print("а) А или В и не С:", A or (B and (not C)))           # True or False = True
print("б) не А и не В:", (not A) and (not B))               # False and True = False
print("в) не (А и С) или В:", (not (A and C)) or B)         # True or False = True
print("г) А и не В или С:", (A and (not B)) or C)           # True or False = True
print("д) А и (не В или С):", A and ((not B) or C))         # True and (True or False) = True
print("е) А или (не (В и С)):", A or (not (B and C)))       # True or True = True
print()

# П3.6
X = False
Y = False
Z = True

print("П3.6:")
print("а) X или Y и не Z:", X or (Y and (not Z)))           # False or False = False
print("б) не X и не Y:", (not X) and (not Y))               # True and True = True
print("в) не (X и Z) или Y:", (not (X and Z)) or Y)         # True or False = True
print("г) X и не Y или Z:", (X and (not Y)) or Z)           # False or True = True
print("д) X и (не Y или Z):", X and ((not Y) or Z))         # False and (True or True) = False
print("е) X или (не (Y или Z)):", X or (not (Y or Z)))      # False or False = False
print()

# П3.7
A = True
B = False
C = False

print("П3.7:")
print("а) А или не (А и В) или С:", A or (not (A and B)) or C)  # True or True or False = True
print("б) не А или А и (В или С):", (not A) or (A and (B or C))) # False or (True and False) = False
print("в) (А или В и не С) и С:", ((A or (B and (not C))) and C) # (True or False) and False = False
print()

