def time_until_overlap(h, m):
    while True:
        m += 1
        if m >= 60:
            m = 0
            h += 1
            if h > 12:
                h = 1
        minute_angle = 6 * m
        hour_angle = (30 * h) + (m * 0.5)
        if hour_angle == minute_angle:
            return h * 60 + m

