var
  sequence: string;
  i: integer;
  corrected: string;
begin
  write('Введите последовательность слов: ');
  readln(sequence);
  
  corrected := '';
  i := 1;
  while i <= length(sequence) do
  begin
    if (i < length(sequence)) and 
       (((sequence[i] = 'ч') and (sequence[i+1] = 'я')) or
        ((sequence[i] = 'щ') and (sequence[i+1] = 'я'))) then
    begin
      // Заменяем "чя" на "ча" и "щя" на "ща"
      corrected := corrected + sequence[i] + 'а';
      i := i + 2;
    end
    else
    begin
      corrected := corrected + sequence[i];
      i := i + 1;
    end;
  end;
  
  writeln('Исправленная последовательность: ', corrected);
end.
