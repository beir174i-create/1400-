13.52. Проверка ряда костей домино
python
# Случай 1: левая и правая половины в разных таблицах
left_half = [4, 3, 0, 6, 1, 2, 5]
right_half = [3, 3, 3, 0, 5, 4, 2]

# Случай 2: каждая таблица может содержать любую половину
table1 = [4, 3, 0, 6, 1, 2, 5]
table2 = [3, 3, 3, 0, 5, 4, 2]

def check_domino_case1(left, right):
    """Проверка для случая 1"""
    for i in range(len(left) - 1):
        if right[i] != left[i + 1]:
            return False, i + 1  # номер кости, нарушающей правила
    return True, None

def check_domino_case2(table1, table2):
    """Проверка для случая 2"""
    # Используем массив записей
    dominoes = []
    for i in range(len(table1)):
        dominoes.append({
            'left': table1[i],
            'right': table2[i],
            'value1': table1[i],
            'value2': table2[i]
        })
    
    for i in range(len(dominoes) - 1):
        current = dominoes[i]
        next_dom = dominoes[i + 1]
        
        # Проверяем все возможные комбинации
        match_found = (current['right'] == next_dom['left'] or
                      current['right'] == next_dom['right'] or
                      current['left'] == next_dom['left'] or
                      current['left'] == next_dom['right'])
        
        if not match_found:
            return False, i + 1
    
    return True, None

# Проверка для случая 1
print("Случай 1 (определенные левая/правая половины):")
valid1, error1 = check_domino_case1(left_half, right_half)
if valid1:
    print("Ряд соответствует правилам домино")
else:
    print(f"Ошибка на кости номер {error1}")

# Проверка для случая 2
print("\nСлучай 2 (любая половина в любой таблице):")
valid2, error2 = check_domino_case2(table1, table2)
if valid2:
    print("Ряд соответствует правилам домино")
else:
    print(f"Ошибка на кости номер {error2}")
