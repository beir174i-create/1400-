    suitcase_dimensions = sorted([a1, a2, a3])
    return (box_dimensions[0] <= suitcase_dimensions[0] and
            box_dimensions[1] <= suitcase_dimensions[1] and
            box_dimensions[2] <= suitcase_dimensions[2])

