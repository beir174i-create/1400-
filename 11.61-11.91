def task_11_91(weights):
    """Средняя масса полных людей (>100 кг) и остальных"""
    if len(weights) != 25:
        return "Массив должен содержать массу 25 человек"
    
    overweight = [w for w in weights if w > 100]
    normal = [w for w in weights if w <= 100]
    
    avg_overweight = sum(overweight) / len(overweight) if overweight else 0
    avg_normal = sum(normal) / len(normal) if normal else 0
    
    return avg_overweight, avg_normal, len(overweight), len(normal)
