def analyze_three_digit_number(n):
    hundreds = n // 100
    tens = (n // 10) % 10
    units = n % 10
    digit_sum = hundreds + tens + units
    product = hundreds * tens * units
    return 10 <= digit_sum < 100, 100 <= product < 1000, product > n, digit_sum % 5 == 0, digit_sum % n == 0

