def reverse_four_digit_number(n):
    return int(str(n)[::-1])

# b)
def swap_first_and_last_four_digits(n):
    return int(str(n)[1:3] + str(n)[0] + str(n)[3])

# c)
def swap_second_and_third_four_digits(n):
    return int(str(n)[0] + str(n)[2] + str(n)[1] + str(n)[3])

# d)
def swap_edges_four_digits(n):
    return int(str(n)[2:4] + str(n)[:2])

