11.163. Удалить элементы по условию
python
# а) удалить все отрицательные элементы
def remove_all_negative(arr):
    i = 0
    count = 0
    n = len(arr)
    
    while i < n - count:
        if arr[i] < 0:
            # Смещаем элементы
            for j in range(i, n-1-count):
                arr[j] = arr[j+1]
            arr[n-1-count] = 0
            count += 1
        else:
            i += 1
    
    return arr

# б) удалить все элементы, большие заданного числа
def remove_greater_than(arr, x):
    i = 0
    count = 0
    n = len(arr)
    
    while i < n - count:
        if arr[i] > x:
            for j in range(i, n-1-count):
                arr[j] = arr[j+1]
            arr[n-1-count] = 0
            count += 1
        else:
            i += 1
    
    return arr

# в) удалить элементы с n1 по n2
def remove_range(arr, n1, n2):
    if 0 <= n1 <= n2 < len(arr):
        count = n2 - n1 + 1
        for i in range(n1, len(arr)-count):
            arr[i] = arr[i+count]
        # Обнуляем последние элементы
        for i in range(len(arr)-count, len(arr)):
            arr[i] = 0
    return arr
