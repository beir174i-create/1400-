def task_11_67(residents):
    """На какой стороне улицы проживает больше жителей"""
    odd_houses = 0   # нечетные номера домов
    even_houses = 0  # четные номера домов
    
    for i in range(len(residents)):
        if (i + 1) % 2 != 0:  # нечетные номера
            odd_houses += residents[i]
        else:  # четные номера
            even_houses += residents[i]
    
    if odd_houses > even_houses:
        return "Больше жителей на стороне с нечетными номерами", odd_houses, even_houses
    elif even_houses > odd_houses:
        return "Больше жителей на стороне с четными номерами", even_houses, odd_houses
    else:
        return "Количество жителей на обеих сторонах одинаково", odd_houses, even_houses
