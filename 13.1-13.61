print(f"Игр с разностью мячей >= 3: {games_big_difference}")

# г) Статистика каждой команды
print("\nСтатистика команд:")
for i in range(n):
    wins = draws = losses = 0
    for j in range(n):
        if i != j:
            goals_for, goals_against = detailed_results[i][j-1 if j>i else j]
            if goals_for > goals_against:
