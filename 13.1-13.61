    if char.isdigit():
        frequency[char] = frequency.get(char, 0) + 1

print("Частота цифр в тексте:")
for digit in sorted(frequency.keys()):
    print(f"{digit}: {frequency[digit]}")

# 2) Все цифры (0-9), включая отсутствующие
print("\nЧастота всех цифр (0-9):")
for digit in '0123456789':
    count = frequency.get(digit, 0)
    print(f"{digit}: {count}")


13.13. Сравнение дат
python
from datetime import date

events = [
    {'название': 'Событие 1', 'год': 1995, 'месяц': 5, 'день': 15},
    {'название': 'Событие 2', 'год': 2000, 'месяц': 8, 'день': 22},
    # ... 20 событий
]

def compare_events(event1, event2):
    # Создаем даты для сравнения
    date1 = date(event1['год'], event1['месяц'], event1['день'])
    date2 = date(event2['год'], event2['месяц'], event2['день'])
    
    if date1 > date2:
        return f"{event1['название']} произошло позже {event2['названия']}"
    elif date2 > date1:
        return f"{event2['название']} произошло позже {event1['названия']}"
    else:
        return "События произошли в один день"

# а) Условный порядковый номер
events_with_index = [(i+1, event) for i, event in enumerate(events)]

def compare_by_index(index1, index2):
    event1 = events[index1-1]
    event2 = events[index2-1]
    return compare_events(event1, event2)

# Пример использования
print(compare_events(events[0], events[1]))
print(compare_by_index(1, 2))
