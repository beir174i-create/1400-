        if data['столица'] == capital:
            return country
    return "Столица не найдена"

# 3) Вся информация о государстве
def get_country_info(country):
    data = countries_data.get(country)
    if data:
        return (f"Государство: {country}\n"
                f"Столица: {data['столица']}\n"
                f"Часть света: {data['часть_света']}\n"
                f"Население: {data['население']} млн чел.\n"
                f"Площадь: {data['площадь']} тыс. кв. км")
    return "Государство не найдено"

# 4) Страны в заданной части света
def get_countries_by_continent(continent):
    result = []
    for country, data in countries_data.items():
        if continent in data['часть_света']:
            result.append(country)
    return result

# 5) Плотность населения для каждого государства
def calculate_population_density():
    densities = {}
    for country, data in countries_data.items():
        if data['площадь'] > 0:
            density = data['население'] / data['площадь']  # млн/тыс.км² = тыс.чел/км²
            densities[country] = density
    return densities

# 6) Количество государств в части света
def count_countries_by_continent(continent):
    count = 0
    for data in countries_data.values():
        if continent in data['часть_света']:
            count += 1
    return count

# 7) Общая площадь в части света
def total_area_by_continent(continent):
    total = 0
    for data in countries_data.values():
        if continent in data['часть_света']:
            total += data['площадь']
    return total

