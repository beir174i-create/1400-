    max_worker = 1
    for i in range(1, workers):
        if salaries[i][j] > max_salary:
            max_salary = salaries[i][j]
            max_worker = i + 1
    print(f"Месяц {j+1}: работник {max_worker} ({max_salary:.2f})")
9.12. Количество учеников в классах
python
parallels = 11
classes = 4
students = [[0] * classes for _ in range(parallels)]

# Ввод данных
print("Ввод количества учеников:")
for i in range(parallels):
    print(f"\nПараллель {i+1}:")
    for j in range(classes):
        count = int(input(f"Класс {chr(65+j)}: "))
        students[i][j] = count

# а) Самый малочисленный класс
min_students = students[0][0]
for i in range(parallels):
    for j in range(classes):
        if students[i][j] < min_students:
            min_students = students[i][j]
print(f"а) В самом малочисленном классе: {min_students} учеников")

# б) Минимальное количество учеников в параллели
parallel_totals = []
for i in range(parallels):
    parallel_total = sum(students[i])
    parallel_totals.append(parallel_total)
min_parallel_total = min(parallel_totals)
print(f"б) Минимальное количество учеников в параллели: {min_parallel_total}")

# в) Минимальное количество учеников в классах с каждой буквой
class_totals = [0] * classes
for j in range(classes):
    for i in range(parallels):
        class_totals[j] += students[i][j]

min_class_total = min(class_totals)
letter_index = class_totals.index(min_class_total)
letter = chr(65 + letter_index)
print(f"в) Минимальное количество учеников в классах '{letter}': {min_class_total}")
9.13. Самые малочисленные классы по разным критериям
python
parallels = 11
classes = 4
students = [[0] * classes for _ in range(parallels)]

# Ввод данных
print("Ввод количества учеников:")
for i in range(parallels):
    print(f"\nПараллель {i+1}:")
    for j in range(classes):
        count = int(input(f"Класс {chr(65+j)}: "))
        students[i][j] = count

# а) Самый малочисленный класс в каждой параллели
print("\nа) Самый малочисленный класс в каждой параллели:")
for i in range(parallels):
    min_in_parallel = min(students[i])
    class_index = students[i].index(min_in_parallel)
    class_letter = chr(65 + class_index)
    print(f"Параллель {i+1}: класс {class_letter} ({min_in_parallel} учеников)")

# б) Самый малочисленный класс среди классов с каждой буквой
print("\nб) Самый малочисленный класс среди классов с каждой буквой:")
for j in range(classes):
    min_for_letter = students[0][j]
    min_parallel = 1
    for i in range(1, parallels):
        if students[i][j] < min_for_letter:
            min_for_letter = students[i][j]
            min_parallel = i + 1
    
    class_letter = chr(65 + j)
    print(f"Классы '{class_letter}': параллель {min_parallel} ({min_for_letter} учеников)")
9.14. Доход магазинов
python
shops = 3
days = 10
income = [[0] * days for _ in range(shops)]

# Ввод данных
