    avg_compulsory = sum(compulsory_scores) / len(compulsory_scores)
    avg_free = sum(free_scores) / len(free_scores)
    
    if avg_compulsory > avg_free:
        return "Лучший результат в обязательной программе", avg_compulsory, avg_free
    elif avg_free > avg_compulsory:
        return "Лучший результат в произвольной программе", avg_free, avg_compulsory
    else:
        return "Результаты равны в обеих программах", avg_compulsory, avg_free
