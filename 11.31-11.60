def task_11_60(arr, divisor, a=None, b=None):
    """а) Сумма нечетных элементов
       б) Сумма элементов, кратных заданному числу
       в) Сумма элементов, кратных a или b"""
    
    # а)
    sum_odd = sum(x for x in arr if x % 2 != 0)
    
    # б)
    sum_divisible = sum(x for x in arr if x % divisor == 0)
    
    # в)
    if a is not None and b is not None:
        sum_divisible_a_b = sum(x for x in arr if x % a == 0 or x % b == 0)
    else:
        sum_divisible_a_b = None
    
    return sum_odd, sum_divisible, sum_divisible_a_b
