11.161. Удалить максимальный и минимальный элементы
python
def remove_max_min_unique(arr):
    if arr and len(set(arr)) == len(arr):
        # Находим индексы
        max_index = arr.index(max(arr))
        min_index = arr.index(min(arr))
        
        # Удаляем сначала больший индекс, чтобы не сбились индексы
        if max_index > min_index:
            # Удаляем максимальный
            for i in range(max_index, len(arr)-1):
                arr[i] = arr[i+1]
            arr[-1] = 0
            
            # Корректируем индекс минимального
            if min_index > max_index:
                min_index -= 1
            
            # Удаляем минимальный
            for i in range(min_index, len(arr)-2):
                arr[i] = arr[i+1]
            arr[-2] = 0
        else:
            # Удаляем минимальный
            for i in range(min_index, len(arr)-1):
                arr[i] = arr[i+1]
            arr[-1] = 0
            
            # Корректируем индекс максимального
            if max_index > min_index:
                max_index -= 1
            
            # Удаляем максимальный
            for i in range(max_index, len(arr)-2):
                arr[i] = arr[i+1]
            arr[-2] = 0
    
    return arr[:23]  # возвращаем 23 элемента
