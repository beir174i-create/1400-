var
  sentence: string;
  i, count: integer;
  inWord: boolean;
begin
  write('Введите предложение: ');
  readln(sentence);
  
  // Случай 1: без начальных и конечных пробелов
  count := 0;
  inWord := false;
  
  for i := 1 to length(sentence) do
  begin
    if sentence[i] <> ' ' then
    begin
      if not inWord then
      begin
        count := count + 1;
        inWord := true;
      end;
    end
    else
      inWord := false;
  end;
  
  writeln('Случай 1 (без учета начальных/конечных пробелов): ', count);
  
  // Случай 2: с начальными и конечными пробелами
  // Удаляем начальные пробелы
  while (length(sentence) > 0) and (sentence[1] = ' ') do
    delete(sentence, 1, 1);
    
  // Удаляем конечные пробелы  
  while (length(sentence) > 0) and (sentence[length(sentence)] = ' ') do
    delete(sentence, length(sentence), 1);
    
  if length(sentence) > 0 then
  begin
    count := 1;
    for i := 1 to length(sentence) do
      if sentence[i] = ' ' then
        count := count + 1;
    writeln('Случай 2 (с учетом начальных/конечных пробелов): ', count);
  end
  else
    writeln('Случай 2: предложение пустое');
end.
