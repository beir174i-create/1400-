    if len(heights) == 25:
        indices = []
        
        # Находим индексы
        if height1 in heights:
            indices.append(heights.index(height1))
        if height2 in heights:
            indices.append(heights.index(height2))
        
        # Сортируем по убыванию и удаляем
        indices.sort(reverse=True)
        for idx in indices:
            for i in range(idx, len(heights)-1):
