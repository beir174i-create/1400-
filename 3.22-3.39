def sum_of_digits(n):
    sum_digits = 0
    while n > 0:
        sum_digits += n % 10
        n //= 10
    return sum_digits

# Для 4-значного
def sum_of_4_digit_number(n):
    return sum_of_digits(n)

# Для 5-значного
def sum_of_5_digit_number(n):
    return sum_of_digits(n)

