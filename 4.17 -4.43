def analyze_four_digit_number(n):
    a = n // 1000
    b = (n // 100) % 10
    c = (n // 10) % 10
    d = n % 10
    digit_sum = a + b + c + d
    product = a * b * c * d
    return a + b == c + d, digit_sum % 3 == 0, product % 4 == 0

