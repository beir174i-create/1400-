count = 0
found = set()

for a in range(1, int(v**(1/3)) + 1):
    if v % a == 0:
        v_ab = v // a
        for b in range(a, int(v_ab**0.5) + 1):
            if v_ab % b == 0:
                c = v_ab // b
                if b <= c:  # чтобы избежать дубликатов
                    # Создаем отсортированный кортеж для уникальности
                    dimensions = tuple(sorted((a, b, c)))
