12.37
pascal
var
  word, first3, last3, middle: string;
  i: integer;
begin
  write('Введите слово: ');
  readln(word);
  
  if length(word) >= 6 then
  begin
    // Способ 1: без цикла
    first3 := copy(word, 1, 3);
    last3 := copy(word, length(word)-2, 3);
    middle := copy(word, 4, length(word)-6);
    writeln('Способ 1: ', last3 + middle + first3);
    
    // Способ 2: с циклом
    first3 := '';
    last3 := '';
    for i := 1 to 3 do
    begin
      first3 := first3 + word[i];
      last3 := word[length(word)-3+i] + last3;
    end;
    middle := '';
    for i := 4 to length(word)-3 do
      middle := middle + word[i];
    writeln('Способ 2: ', last3 + middle + first3);
  end
  else
    writeln('Слово слишком короткое');
end.
12.38
pascal
var
  word, part1, part2: string;
  k, i: integer;
begin
  write('Введите слово: ');
  readln(word);
  write('Введите k: ');
  readln(k);
  
  if (k >= 1) and (k <= length(word)) then
  begin
    // Способ 1: без цикла
    part1 := copy(word, 1, k);
    part2 := copy(word, k+1, length(word)-k);
    writeln('Способ 1: ', part2 + part1);
    
    // Способ 2: с циклом
    part1 := '';
    part2 := '';
    for i := 1 to k do
      part1 := part1 + word[i];
    for i := k+1 to length(word) do
      part2 := part2 + word[i];
    writeln('Способ 2: ', part2 + part1);
  end
  else
    writeln('Некорректное значение k');
end.
