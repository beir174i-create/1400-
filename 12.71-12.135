var
  sentence, word: string;
  i, wordCount: integer;
begin
  write('Введите предложение (слова через 2 пробела, не менее 4 слов): ');
  readln(sentence);
  
  wordCount := 0;
  word := '';
  i := 1;
  
  // Пропускаем возможные пробелы в начале
  while (i <= length(sentence)) and (sentence[i] = ' ') do
    i := i + 1;
    
  while (i <= length(sentence)) and (wordCount < 3) do
  begin
    if sentence[i] <> ' ' then
      word := word + sentence[i]
    else
    begin
      if word <> '' then
      begin
        wordCount := wordCount + 1;
        case wordCount of
          1: writeln('Первое слово: ', word);
          2: writeln('Второе слово: ', word);
          3: writeln('Третье слово: ', word);
        end;
        word := '';
      end;
      // Пропускаем двойные пробелы
      while (i <= length(sentence)) and (sentence[i] = ' ') do
        i := i + 1;
      continue;
    end;
    i := i + 1;
  end;
end.
