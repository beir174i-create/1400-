var
  text: string;
  i, count, firstSentenceEnd: integer;
begin
  write('Введите текст: ');
  readln(text);
  
  // Находим конец первого предложения
  firstSentenceEnd := 0;
  for i := 1 to length(text) do
    if (text[i] = '.') or (text[i] = '!') or (text[i] = '?') then
    begin
      firstSentenceEnd := i;
      break;
    end;
  
  // Случай 1: известно, что буквы и есть
  if firstSentenceEnd > 0 then
  begin
    count := 0;
    for i := 1 to firstSentenceEnd do
      if text[i] = 'и' then
        count := count + 1;
    writeln('Случай 1 (буквы "и" есть): ', count, ' букв "и"');
  end;
  
  // Случай 2: букв и может не быть
  count := 0;
  if firstSentenceEnd = 0 then
    firstSentenceEnd := length(text);  // если нет конца предложения, берем весь текст
    
  for i := 1 to firstSentenceEnd do
    if text[i] = 'и' then
      count := count + 1;
      
  if count > 0 then
    writeln('Случай 2 (букв "и" может не быть): ', count, ' букв "и"')
  else
    writeln('Случай 2 (букв "и" может не быть): букв "и" нет');
