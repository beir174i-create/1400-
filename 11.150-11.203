def fix_speed_order(arr, k):
    if len(arr) == 30 and 0 <= k < 30:
        value = arr[k]
        
        # Проверяем, нарушает ли элемент упорядоченность
        if (k > 0 and value < arr[k-1]) or (k < 29 and value > arr[k+1]):
            # Удаляем элемент
            for i in range(k, len(arr)-1):
                arr[i] = arr[i+1]
            arr[-1] = 0
            
            # Находим правильное место
            pos = 0
            while pos < len(arr)-1 and arr[pos] < value:
                pos += 1
            
            # Вставляем на правильное место
            for i in range(len(arr)-2, pos-1, -1):
                arr[i] = arr[i-1]
            arr[pos] = value
    
    return arr
