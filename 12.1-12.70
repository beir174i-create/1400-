var
  sentence, sub: string;
  i, count: integer;
begin
  write('Введите предложение: ');
  readln(sentence);
  
  // а) буквосочетания "ро"
  count := 0;
  for i := 1 to length(sentence)-1 do
    if (sentence[i] = 'р') and (sentence[i+1] = 'о') then
      count := count + 1;
  writeln('а) Количество "ро": ', count);
  
  // б) любое буквосочетание из 2 букв
  write('Введите буквосочетание из 2 букв: ');
  readln(sub);
  
  count := 0;
  for i := 1 to length(sentence)-1 do
    if copy(sentence, i, 2) = sub then
      count := count + 1;
  writeln('б) Количество "', sub, '": ', count);
  
  // в) любое буквосочетание
  write('Введите буквосочетание: ');
  readln(sub);
  
  count := 0;
  for i := 1 to length(sentence)-length(sub)+1 do
    if copy(sentence, i, length(sub)) = sub then
      count := count + 1;
  writeln('в) Количество "', sub, '": ', count);
end.
