print("Пары дружественных чисел меньше 50000:")

# Функция для вычисления суммы делителей
def sum_of_divisors(n):
    total = 0
    for i in range(1, n):
        if n % i == 0:
            total += i
    return total

pairs_found = []
for a in range(1, 50000):
    b = sum_of_divisors(a)
    if b > a and b < 50000:
        if sum_of_divisors(b) == a:
            pairs_found.append((a, b))
            print(f"{a} и {b}")

print(f"\nВсего найдено {len(pairs_found)} пар дружественных чисел")

# Вывод с пояснениями
for a, b in pairs_found:
    print(f"\nПара {a} и {b}:")
    print(f"  Делители {a}: ", end="")
    div_a = [i for i in range(1, a) if a % i == 0]
    print(f"{div_a}, сумма = {sum(div_a)} = {b}")
    print(f"  Делители {b}: ", end="")
    div_b = [i for i in range(1, b) if b % i == 0]
    print(f"{div_b}, сумма = {sum(div_b)} = {a}")
