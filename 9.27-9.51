for num in range(100, 301):
    divisor_sum = 0
    # Находим все делители и их сумму
    for i in range(1, num // 2 + 1):  # проверяем только до num/2
        if num % i == 0:
            divisor_sum += i
            # Если делитель не является квадратным корнем, добавляем и парный делитель
            if i != num // i and i * i != num:
                # Но в этом цикле мы идем только до num/2, так что парные делители > num/2 не учитываются
                pass
    
    # Добавляем само число, если оно не является делителем самого себя в обычном смысле
    # Но по условию задачи "сумма всех своих делителей (естественно, кроме самого числа)"
    # так что само число не добавляем
    
    if divisor_sum == 50:
        print(f"{num} (сумма делителей: {divisor_sum})")
        
        # Для проверки выводим делители
        divisors = []
        for i in range(1, num):
            if num % i == 0:
                divisors.append(i)
        print(f"  Делители: {divisors}")
