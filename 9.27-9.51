python
n = int(input("Введите натуральное число: "))

print(f"Все простые делители числа {n}:")

prime_divisors = set()
temp = n
d = 2

while d * d <= temp:
    if temp % d == 0:
        # Проверяем, что d простое
        is_prime = True
        for i in range(2, int(d**0.5) + 1):
            if d % i == 0:
                is_prime = False
                break
        
        if is_prime:
            prime_divisors.add(d)
        temp //= d
    else:
        d += 1

if temp > 1:
    # Проверяем, что temp простое
    is_prime = True
    for i in range(2, int(temp**0.5) + 1):
        if temp % i == 0:
            is_prime = False
            break
    if is_prime:
        prime_divisors.add(temp)

if prime_divisors:
    print("Простые делители:", sorted(prime_divisors))
else:
    print(f"У числа {n} нет простых делителей (оно само простое)")
