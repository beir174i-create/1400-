def task_11_69(arr, divisor=None):
    """а) Количество элементов, отличных от последнего
       б) Количество элементов, кратных divisor"""
    
    # а)
    last_element = arr[-1] if arr else None
    count_diff_from_last = sum(1 for x in arr if x != last_element)
    
    # б)
    if divisor is not None:
        count_divisible = sum(1 for x in arr if x % divisor == 0)
    else:
        count_divisible = None
    
    return count_diff_from_last, count_divisible Задача 11.70
