import math

def can_circle_fit_in_triangle(circle_area, triangle_area):
    circle_radius = math.sqrt(circle_area / math.pi)
    triangle_height = (math.sqrt(3) / 2) * (math.sqrt(4 * triangle_area / math.sqrt(3)))
    return circle_radius <= triangle_height

def can_triangle_fit_in_circle(circle_area, triangle_area):
    circle_radius = math.sqrt(circle_area / math.pi)
    triangle_side = math.sqrt((4 * triangle_area) / math.sqrt(3))
    return triangle_side <= circle_radius * 2

