def task_11_41(arr, s, k):
    """а) Является ли s-й элемент положительным
       б) Является ли k-й элемент четным
       в) Какой элемент больше: k-й или s-й"""
    
    # Проверка корректности индексов
    if not (0 <= s < len(arr) and 0 <= k < len(arr)):
        return None, None, None
    
    # а)
    is_s_positive = arr[s] > 0
    
    # б)
    is_k_even = arr[k] % 2 == 0
    
    # в)
    if arr[k] > arr[s]:
        comparison = f"k-й элемент ({arr[k]}) больше s-го ({arr[s]})"
    elif arr[k] < arr[s]:
        comparison = f"s-й элемент ({arr[s]}) больше k-го ({arr[k]})"
    else:
        comparison = "Элементы равны"
    
    return is_s_positive, is_k_even, comparison

